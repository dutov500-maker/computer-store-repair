{
  "tests": [
    {
      "name": "OPTIONS request for CORS",
      "method": "OPTIONS",
      "path": "/",
      "expectedStatus": 200
    },
    {
      "name": "Reject empty password",
      "method": "POST",
      "path": "/",
      "body": {
        "password": ""
      },
      "expectedStatus": 400
    },
    {
      "name": "Reject invalid password",
      "method": "POST",
      "path": "/",
      "body": {
        "password": "wrongpassword"
      },
      "expectedStatus": 401
    }
  ]
}
